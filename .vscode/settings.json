{
  "deno.enable": true,
  "deno.lint": true,
  "editor.formatOnSave": true,
  "json.format.enable": true,
  "editor.indentSize": "tabSize",
  "editor.tabSize": 2,
  "[jsonc]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  }
}
