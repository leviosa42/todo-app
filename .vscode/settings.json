  "deno.config": "deno.jsonc",
  "deno.enable": true,
  "deno.lint": true,
  "editor.formatOnSave": true,
  "json.format.enable": true,
  "editor.detectIndentation": true,
  "[jsonc]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
